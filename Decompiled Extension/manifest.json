{
  "name": "Unbounce Dev Favicon",
  "description": "Change the favicon to a green icon on the local dev environment",
  "version": "1.1",
    "permissions": [
      "http://localhost/*"
    ],
    "content_scripts": [
      {
        "matches": ["http://localhost/*"],
        "js": ["popup-local.js"]
      },
      {
        "matches": ["http://app-test.unbounce.com/*"],
        "matches": ["https://app-test.unbounce.com/*"],
        "js": ["popup-local.js"]
      },
      {
        "matches": ["http://app-test-builder.unbounce.com/*"],
        "matches": ["https://app-test-builder.unbounce.com/*"],
        "js": ["popup-local.js"]
      }

    ],"web_accessible_resources": [
      "dev.ico"
    ],"icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" },
  "manifest_version": 2
}